{
  "name": "hooked-client-web",
  "author": "Brian Dombrowski <bdombro@gmail.com>",
  "private": false,
  "version": "0.0.1",
  "scripts": {
    "types:generate": "rm src/@types/__generated__/*; apollo client:codegen --localSchemaFile ../postgraphile/__generated__/schema.json --includes 'src/**' --target typescript --outputFlat src/@types/__generated__/",
    "types:generate:watch": "rm src/@types/__generated__/*; apollo client:codegen --watch --localSchemaFile ../postgraphile/__generated__/schema.json --includes 'src/**' --target typescript --outputFlat src/@types/__generated__/",
    "types": "tsc --noEmit",
    "types:watch": "tsc --noEmit --watch",
    "dev": "yarn clean && webpack-dev-server --mode development --port 3001 --progress --inline --hot --colors",
    "lerna:dev": "yarn dev",
    "build": "yarn clean && webpack -p --progress --colors --display=minimal",
    "lerna:build": "yarn build",
    "clean": "if [ -d ./build ]; then rm -rf build; fi; cp -rf src/static build",
    "lerna:clean": "yarn clean",
    "clear-cache": "gulp clear-cache",
    "start": "yarn build"
  },
