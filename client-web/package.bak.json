{
  "name": "hooked-client-web",
  "author": "Brian Dombrowski <bdombro@gmail.com>",
  "private": false,
  "version": "0.0.1",
  "scripts": {
    "upgrade-hard": "npx ncu -u && yarn && yarn upgrade",
    "types:generate": "rm src/@types/__generated__/*; apollo client:codegen --localSchemaFile ../postgraphile/__generated__/schema.json --includes 'src/**' --target typescript --outputFlat src/@types/__generated__/",
    "types:generate:watch": "rm src/@types/__generated__/*; apollo client:codegen --watch --localSchemaFile ../postgraphile/__generated__/schema.json --includes 'src/**' --target typescript --outputFlat src/@types/__generated__/",
    "types": "tsc --noEmit",
    "types:watch": "tsc --noEmit --watch",
    "dev": "webpack-dev-server --mode development --port 3001 --progress --inline --hot --colors",
    "lerna:dev": "yarn dev",
    "build": "rm -rf build && cp -rf src/static build && NODE_ENV=production webpack -p --progress --colors --display=errors-only --quiet",
    "lerna:build": "yarn build",
    "clean": "rm -rf build node_modules; cp -rf src/static build",
    "lerna:clean": "yarn clean",
    "start": "yarn build",
    "profile": "webpack --profile --json > webpack-profile.json; # See https://webpack.js.org/guides/code-splitting/#bundle-analysis",
    "analyze": "ANALYZE=true yarn build; # creates and opens an analysis bundle in browser"
  },
