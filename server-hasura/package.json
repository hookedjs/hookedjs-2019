{
  "name": "hookedjs-server-hasura",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "docker-compose up --build",
    "dev": "docker-compose up --build",
    "lerna:dev": "yarn dev",
    "test": "yarn dev",
    "lerna:test": "yarn test",
    "migrations:apply": "hasura --admin-secret=$SUPERUSER_PASSWORD migrate apply",
    "clean": "docker stop $(docker ps -a -q); docker rm $(docker ps -a -q); docker volume prune -f",
    "lerna:clean": "yarn clean"
  }
}
