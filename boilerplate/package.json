{
  "name": "hookedjs-project-boilerplate",
  "description": "A starter project for HookedJS",
  "author": "Brian Dombrowski <bdombro@gmail.com>",
  "version": "0.0.2",
  "license": "ISC",
  "scripts": {
    "upgrade-hard": "npx ncu -u && yarn && yarn upgrade",
    "prettier": "prettier --loglevel warn --write \"{client-web/src,server-koa2/src}/**/*.{ts,tsx,css,json,md,js}\"",
    "types": "tsc --noEmit",
    "types:watch": "tsc --noEmit --watch",
    "start": "hookedjs -s",
    "build": "hookedjs -b",
    "dev": "hookedjs -d",
    "test": "hookedjs -t",
    "clean": "rm -rf build node_modules"
  },
  "private": false,
  "husky": {
    "hooks": {
      "pre-commit": ""
    }
  },
  "devDependencies": {
    "prettier": "^1.16.4",
    "solidarity": "^2.3.1"
  },
  "dependencies": {
    "hamburgers": "^1.1.3",
    "hookedjs": "~2.0",
    "husky": "^1.3.1",
    "react-responsive-modal": "^3.6.0"
  },
  "engines": {
    "node": ">11.7",
    "npm": ">=9999.0.0",
    "yarn": ">=1.9.4"
  }
}
