{
  "packages": [
    ".",
    "client-web",
    "client-web/src/themes/*",
    "server-hasura",
    "server-koa2",
    "server-postgres"
  ],
  "version": "0.0.1",
  "npmClient": "yarn",
  "command": {
    "publish": {
      "ignoreChanges": [
        "ignored-file",
        "*.md"
      ]
    },
    "bootstrap": {
      "ignore": "component-*",
      "npmClientArgs": [
        "--no-package-lock"
      ]
    }
  }
}
